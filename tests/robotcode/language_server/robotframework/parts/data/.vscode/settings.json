    "robotcode.robot.variables": {
        "CMD_VAR": "1"
    },
    "robotcode.robot.env": {
        "ENV_VAR": "1"
    },
    "robotcode.debug.attachPython": true,
    "robotcode.debug.defaultConfiguration": {
        "pythonConfiguration": {
            "justMyCode": false
        }
    },
    "robotcode.languageServer.args": [
        "--debugpy",
        "--log",
        "--log-level",
        "TRACE"
    ],
